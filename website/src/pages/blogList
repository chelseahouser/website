import React, { Component } from "react";
import Blog from '../components/blog';

class BlogList extends Component {
    constructor(props) {
      super(props);
      this.state = {};
  
      ReactGA.initialize("UA-110570651-1");
      ReactGA.pageview(window.location.pathname);
    }
  
    render() {
      return (
        <div className="App">
          <Blog />
        </div>
      );
    }
  }
  
  export default BlogList;
